{
  "name": "ijspoort",
  "productName": "ijspoort",
  "version": "1.0.0",
  "description": "Entrance identification and registration of club members",
  "main": "src/main/index.js",
  "scripts": {
    "start": "concurrently \"npm:svelte-dev\" \"electron-forge start\"",
    "start_el": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "re": "./node_modules/.bin/electron-rebuild",
    "svelte-build": "rollup -c",
    "sv": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-start": "sirv public"
  },
  "keywords": [],
  "author": {
    "name": "Maarten Vandekeybus",
    "email": "info@martti.be"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "i"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@iconify/icons-fa": "^1.1.0",
    "@iconify/svelte": "^2.0.0",
    "@sveltejs/pancake": "^0.0.18",
    "autocompleter": "^9.3.2",
    "axios": "^1.7.9",
    "bootstrap": "^5",
    "bootswatch": "^5.3.3",
    "dotenv": "^16.4.7",
    "electron": "~34",
    "electron-store": "^10.0.1",
    "lodash": "^4.17.21",
    "mqtt": "^5.10.3",
    "nfc-pcsc": "^0.8.1",
    "node-gyp": "^11.1.0",
    "papaparse": "^5.5.2",
    "rpio": "^2.4.2",
    "svelte": "^3.29.4",
    "sveltestrap": "^5.4.1-0",
    "vanillajs-datepicker": "^1.3.4",
    "xlsx": "^0.18.5"
  },
  "dependencies": {}
}
